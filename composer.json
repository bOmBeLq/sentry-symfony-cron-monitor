{
    "name": "bml/sentry-symfony-cron-monitor",
    "type": "symfony-bundle",
    "description": "Symfony cron monitoring integration for Sentry (http://getsentry.com)",
    "keywords": ["logging", "errors", "symfony", "sentry", "cron"],
    "homepage": "http://getsentry.com",
    "license": "MIT",
    "require": {
        "php": "^7.2||^8.0",
        "sentry/sentry": "^3.20.1",
        "symfony/http-client": "^5.4",
        "nyholm/psr7": "^1.8",
        "symfony/dependency-injection": "^4.4.20||^5.0.11||^6.0",
        "symfony/http-kernel": "^4.4.20||^5.0.11||^6.0",
        "sentry/sentry-symfony": "4.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.19||<=3.16.0",
        "phpunit/phpunit": "^8.5.14||^9.3.9",
        "symfony/phpunit-bridge": "^5.2.6||^6.0",
        "symfony/framework-bundle": "^4.4.20||^5.0.11||^6.0",
        "symfony/yaml": "^4.4.20||^5.0.11||^6.0"
    },
    "autoload": {
        "psr-4": {
            "bmL\\SentryCronMonitorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bmL\\SentryCronMonitorBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "releases/3.2.x": "3.2.x-dev",
            "releases/2.x": "2.x-dev",
            "releases/1.x": "1.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
