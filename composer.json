{
    "name": "bml/sentry-symfony-cron-monitor",
    "type": "symfony-bundle",
    "description": "Symfony cron monitoring integration for Sentry (http://getsentry.com)",
    "keywords": ["logging", "errors", "symfony", "sentry", "cron"],
    "homepage": "http://getsentry.com",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "nyholm/psr7": "^1.8",
        "symfony/dependency-injection": "^5.0.11||^6.0||^7.0",
        "symfony/http-kernel": "^5.0.11||^6.0||^7.0",
        "sentry/sentry-symfony": "^4.0||^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.19||<=3.16.0",
        "phpunit/phpunit": "^8.5.14||^9.3.9",
        "symfony/phpunit-bridge": "^5.2.6||^6.0||^7.0",
        "symfony/framework-bundle": "^4.4.20||^5.0.11||^6.0||^7.0",
        "symfony/yaml": "^4.4.20||^5.0.11||^6.0||^7.0"
    },
    "autoload": {
        "psr-4": {
            "bmL\\SentryCronMonitorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bmL\\SentryCronMonitorBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
